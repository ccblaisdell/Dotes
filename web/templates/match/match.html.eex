  <li>
    <strong>Human_players:</strong>
    <%= @match.human_players %>
  </li>
  <li>
    <strong>Positive_votes:</strong>
    <%= @match.positive_votes %>
  </li>
  <li>
    <strong>Negative_votes:</strong>
    <%= @match.negative_votes %>
  </li>
  <li>
    <strong>Cluster:</strong>
    <%= @match.cluster %>
  </li>
  <li>
    <strong>League_id:</strong>
    <%= @match.league_id %>
  </li>

</ul>
