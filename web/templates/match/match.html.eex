<ul>

  <li>
    <strong>Match ID:</strong>
    <%= @match.id %>
  </li>
  <li>
    <strong>Seq_num:</strong>
    <%= @match.seq_num %>
  </li>
  <li>
    <strong>Start_time:</strong>
    <%= format_time @match.start_time %>
  </li>
  <li>
    <strong>Ended:</strong>
    <%= Dotes.Match.end_time(@match) |> format_time %>
  </li>
  <li>
    <strong>Lobby_type:</strong>
    <%= Dota.LobbyType.get @match.lobby_type %>
  </li>
  <li>
    <strong>Game_mode:</strong>
    <%= Dota.GameMode.get @match.game_mode %>
  </li>
  <li>
    <strong>Radiant_win:</strong>
    <%= @match.radiant_win %>
  </li>
  <li>
    <strong>Duration:</strong>
    <%= @match.duration %>
  </li>

  <li>
    <strong>First_blood_time:</strong>
    <%= @match.first_blood_time %>
  </li>
  <li>
    <strong>Tower_status_dire:</strong>
    <%= @match.tower_status_dire %>
  </li>
  <li>
    <strong>Tower_status_radiant:</strong>
    <%= @match.tower_status_radiant %>
  </li>
  <li>
    <strong>Barracks_status_dire:</strong>
    <%= @match.barracks_status_dire %>
  </li>
  <li>
    <strong>Barracks_status_radiant:</strong>
    <%= @match.barracks_status_radiant %>
  </li>

  <li>
    <strong>Season:</strong>
    <%= @match.season %>
