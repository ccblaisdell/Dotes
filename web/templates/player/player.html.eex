<ul>

  <li>
    <strong>USER:</strong>
    <%= if @player.user_id do %>
      <%= link "user", to: user_path(@conn, :show, @player.user_id) %>
    <% end %>
  </li>

  <li>
    <strong>MATCH:</strong>
    <%= link "match", to: match_path(@conn, :show, @match.id) %>
  </li>
  <li>
    <strong>WON?</strong>
    <%= DotaQuantify.Player.won?(@player, @match) %>
  </li>
  <li>
    <strong>account_id:</strong>
    <%= @player.account_id %>
  </li>
  <li>
  	<strong>assists:</strong>
		<%= @player.assists %>
  </li>
  <li>
  	<strong>deaths:</strong>
		<%= @player.deaths %>
  </li>
  <li>
  	<strong>gold:</strong>
		<%= @player.gold %>
  </li>
  <li>
  	<strong>denies:</strong>
		<%= @player.denies %>
  </li>
  <li>
  	<strong>gold_per_min:</strong>
		<%= @player.gold_per_min %>
  </li>
  <li>
  	<strong>gold_spent:</strong>
		<%= @player.gold_spent %>
  </li>
  <li>
  	<strong>hero_damage:</strong>
		<%= @player.hero_damage %>
  </li>
  <li>
  	<strong>hero_healing:</strong>
		<%= @player.hero_healing %>
  </li>
  <li>
  	<strong>hero:</strong>
		<%= DotaApi.Hero.get @player.hero_id %>
  </li>
  <li>
  	<strong>item_0:</strong>
		<%= DotaApi.Item.get @player.item_0 %>
  </li>
  <li>
  	<strong>item_1:</strong>
		<%= DotaApi.Item.get @player.item_1 %>
  </li>
  <li>
